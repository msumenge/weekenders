<div class="page-content">
	<h3 class="page-title">Settings</h3>
	<ul class="mdl-list settings-list">
		<li class="mdl-list__item">
			<span class="mdl-list__item-primary-content">
				<i class="material-icons">public</i>
				Join the team
			</span>
			<span class="mdl-list__item-secondary-action">
				<label class="mdl-switch mdl-js-switch mdl-js-ripple-effect" for="settings-user-join">
					<input type="checkbox" id="settings-user-join" class="mdl-switch__input" />
				</label>
			</span>
		</li>
		<li class="mdl-list__item">
			<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
				<input class="mdl-textfield__input" type="text" id="settings-user-name" disabled />
				<label class="mdl-textfield__label" for="settings-user-name">Name</label>
			</div>
		</li>
		<li class="mdl-list__item">
			<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
				<input class="mdl-textfield__input" type="text" id="settings-user-phone" pattern="-?[0-9\s]*(\.[0-9\s]+)?" disabled />
				<label class="mdl-textfield__label" for="settings-user-phone">Phone Number</label> <span class="mdl-textfield__error">Please enter a valid number only!</span> </div>
		</li>
		<li class="mdl-list__item">
			<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
				<input class="mdl-textfield__input" type="email" id="settings-user-email" disabled />
				<label class="mdl-textfield__label" for="settings-user-email">Email</label>
			</div>
		</li>
		<li class="mdl-list__item">
			<span class="mdl-list__item-primary-content">
				<i class="material-icons">explore</i>
				Share my location
			</span>
			<span class="mdl-list__item-secondary-action">
				<label class="mdl-switch mdl-js-switch mdl-js-ripple-effect" for="settings-user-location">
					<input type="checkbox" id="settings-user-location" class="mdl-switch__input" disabled />
				</label>
			</span>
		</li>
		<li class="mdl-list__item">
			<span class="mdl-list__item-primary-content">
				<i class="material-icons">adb</i>
				My Icon
			</span>
		</li>
		<li class="mdl-list__item" style="padding-top: 0px;">
			<div class="mdl-grid padding-0 margin-0 fullwidth align-center">
				<% 
					var icons = ['face', 'explore', 'spa', 'blur_on', 'filter_vintage', 'graphic_eq', 'bubble_chart', 'toys', 'camera', 'gesture', 'stars', 'whatshot'];
					for (var i in icons) {
				%>
				<div class="mdl-cell mdl-cell--1-col-phone">
					<button data-icon="<%= icons[i] %>" class="mdl-button mdl-js-button mdl-button--icon settings-user-icon">
						<i class="material-icons"><%= icons[i] %></i>
					</button>
				</div>
				<% } %>
			</div>
		</li>
	</ul>
</div>